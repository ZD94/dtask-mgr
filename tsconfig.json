{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "ES5",
    "downlevelIteration": true,
    "outDir": "tmp/tsout",
    "module": "commonjs",
    "sourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "experimentalDecorators": true,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["es2015", "dom"]
  }
}
