{
  "appName": "${JL_APP_NAME}",
  "rootDir": "${JL_APP_DATA_DIR}",
  "listen": "4000",
  "mgrport": "5000",
  "workdir": "${{rootDir}}/tmp/workdir",

  "postgres":{
    "url": "${JL_APP_POSTGRES}",
    "debug": true
  },
  "api": {
    "debug": true,
    "remotes": []
  },
  "logger": {
    "path": "${{rootDir}}/log",
    "prefix": "${{appName}}_"
  },
  "tasks": {
    "ctrip-flight-abroad": {
      "type": "docker",
      "concurrency": 1,
      "prog": "docker.l.jingli365.com/dtasks-ctrip-flight-abroad",
      "version": "${JL_APP_BRANCH}"
    },
    "ctrip-flight-domestic": {
      "type": "docker",
      "concurrency": 1,
      "prog": "docker.l.jingli365.com/dtasks-ctrip-flight-domestic",
      "version": "${JL_APP_BRANCH}"
    },
    "ctrip-hotel-abroad": {
      "type": "docker",
      "concurrency": 3,
      "prog": "docker.l.jingli365.com/dtasks-ctrip-hotel-abroad",
      "version": "${JL_APP_BRANCH}"
    },
    "ctrip-hotel-domestic": {
      "type": "docker",
      "concurrency": 3,
      "prog": "docker.l.jingli365.com/dtasks-ctrip-hotel-domestic",
      "version": "${JL_APP_BRANCH}"
    },
    "ctrip-train-eurorail": {
      "type": "docker",
      "concurrency": 3,
      "prog": "docker.l.jingli365.com/dtasks-ctrip-train-eurorail",
      "version": "${JL_APP_BRANCH}"
    },
    "ctrip-train-domestic": {
      "type": "docker",
      "concurrency": 3,
      "prog": "docker.l.jingli365.com/dtasks-ctrip-train-domestic",
      "version": "${JL_APP_BRANCH}"
    },
    "qunar-train-domestic": {
      "type": "docker",
      "concurrency": 3,
      "prog": "docker.l.jingli365.com/dtasks-qunar-train-domestic",
      "version": "${JL_APP_BRANCH}"
    },
    "skyscanner-flight": {
      "type": "docker",
      "concurrency": 1,
      "prog": "docker.l.jingli365.com/dtasks-skyscanner-flight-abroad",
      "version": "${JL_APP_BRANCH}"
    },
    "jingzhong-flight": {
      "type": "docker",
      "concurrency": 10,
      "prog": "docker.l.jingli365.com/dtasks-jingzhong-flight",
      "version": "${JL_APP_BRANCH}"
    },
    "phantom-ctrip-flight": {
      "type": "docker",
      "concurrency": 5,
      "prog": "docker.l.jingli365.com/dtasks-phantom-ctrip-flight",
      "version": "${JL_APP_BRANCH}"
    }
  }
}
