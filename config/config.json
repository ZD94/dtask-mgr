{
  "appName": "${JL_APP_NAME}",
  "rootDir": "${JL_APP_DATA_DIR}",
  "listen": "4000",

  "workdir": "${{rootDir}}/tmp/workdir",

  "postgres":{
    "url": "${JL_APP_POSTGRES}",
    "debug": true
  },
  "api": {
    "debug": true,
    "remotes": []
  },
  "logger": {
    "path": "${{rootDir}}/log",
    "prefix": "${{appName}}_"
  },
  "tasks": {
    "ctrip-train-domestic": {
      "type": "docker",
      "concurrency": 2,
      "prog": "docker.l.jingli365.com/dtasks-dtask-ctrip-train-domestic",
      "version": "master"
    },
    "ctrip-hotel-abroad": {
      "type": "docker",
      "concurrency": 2,
      "prog": "docker.l.jingli365.com/dtasks-dtask-ctrip-hotel-abroad",
      "version": "master"
    },
    "ctrip-eurorail": {
      "type": "docker",
      "concurrency": 2,
      "prog": "docker.l.jingli365.com/dtasks-dtask-ctrip-eurorail",
      "version": "master"
    },
    "ctrip-flight-abroad": {
      "type": "docker",
      "concurrency": 2,
      "prog": "docker.l.jingli365.com/dtasks-dtask-ctrip-flight-abroad",
      "version": "master"
    },
    "ctrip-flight-domestic": {
      "type": "docker",
      "concurrency": 2,
      "prog": "docker.l.jingli365.com/dtasks-dtask-ctrip-flight-domestic",
      "version": "master"
    },
    "qunar-train-domestic": {
      "type": "docker",
      "concurrency": 2,
      "prog": "docker.l.jingli365.com/dtasks-dtask-qunar-train-domestic",
      "version": "master"
    },
    "ctrip-hotel-domestic": {
      "type": "docker",
      "concurrency": 2,
      "prog": "docker.l.jingli365.com/dtasks-dtask-ctrip-hotel-domestic",
      "version": "master"
    }
  }
}
